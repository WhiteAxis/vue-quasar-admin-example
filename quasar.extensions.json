{
  "@quasar/testing-unit-jest": {
    "babel": "babelrc",
    "options": [
      "SFC",
      "scripts"
    ]
  },
  "@quasar/testing-e2e-cypress": {
    "options": [
      "scripts"
    ]
  },
  "@quasar/testing-quality": {
    "options": [
      "scripts"
    ]
  },
  "@quasar/testing-security": {
    "options": [
      "scripts"
    ]
  },
  "@quasar/testing": {
    "harnesses": [
      "unit-jest",
      "e2e-cypress",
      "quality",
      "security"
    ]
  },
  "@quasar/dotenv": {
    "env_development": ".env",
    "env_production": ".env-prod",
    "common_root_object": "none",
    "create_env_files": true,
    "add_env_to_gitignore": true
  }
}